<?xml version="1.0" encoding="UTF-8"?>
<plugin title="CleanDash" loader="DEFAULT">
<overlays>
		<overlay id="startup.CleanDash" resourcebundle="content/CleanDash/resources/lang/messages">
			<menubar id="toolsmenu">
	        	<menuitem id="CleanDash" label="${Launcher.CleanDash}" command="mantleXulHandler.openUrl('${Launcher.CleanDash}','${Launcher.CleanDash_TOOLTIP}','plugin/CleanDash/api/default')"/>
	      	</menubar>
	    </overlay>
	</overlays>
	<lifecycle-listener class="pt.webdetails.cpk.CpkLifecycleListener"/>
	<static-paths>
		<static-path url="/CleanDash/static" localFolder="static"/>
		<static-path url="/CleanDash/resources" localFolder="resources"/>
	</static-paths>
	<content-types>
		<content-type>
			<title>CleanDash</title>
			<description>not defined</description>
			<company name="not defined" url="not defined" logo="@PLUGIN_COMPANY_LOGO@"></company>
		</content-type>
	</content-types>
	<content-generator
		id="CleanDash"
		title="CleanDash"
		type="CleanDash"
		class="pt.webdetails.cpk.CpkContentGenerator"/>
	<!-- Menu entry -->
	<menu-items>
		<!--<menu-item id="CleanDash_main" anchor="tools-submenu" label="CleanDash" command="content/CleanDash/" type="MENU_ITEM" how="LAST_CHILD"/>-->
	</menu-items>
</plugin>
